{
  "manifest_version": 2,
  "name": "*PACKAGENAME*",
  "description": "*DESCRIPTION*",
  "version": "*VERSION*",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications",
    "videoCapture",
    "webview"
  ],
  "app": {
    "background": {
      "scripts": ["initial.js"]
    }
  },
  "icons": {
    "16":  "img/app/16x16.png",
    "48":  "img/app/48x48.png",
    "128": "img/app/128x128.png"
  }
}
